{
  "name": "@anissoft/react-conditions",
  "version": "1.0.0",
  "description": "Conditional rendering for react",
  "keywords": [
    "react",
    "if"
  ],
  "author": "Alexey <Anisimov>",
  "homepage": "https://github.com/Anissoft/js-libs#readme",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "lib/react-conditions.js",
  "typings": "lib/react-conditions",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "prepare": "npm run build",
    "prepublish": "npm run test:coverage",
    "test": "jest --env=jsdom --silent",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "jest --watch --env=jsdom"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anissoft/js-libs.git"
  },
  "bugs": {
    "url": "https://github.com/Anissoft/js-libs/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": ".*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "!**/*.test.(ts|tsx)",
      "src/**/*"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^26.0.16",
    "jest": "^26.6.3",
    "jest-config": "^25.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.4.1",
    "typescript": "^4.1.2"
  },
  "gitHead": "30e051da6bbbf4279490b0eaf17f5f4d708ce36a"
}
