{
  "name": "@anissoft/react-di",
  "version": "1.0.3",
  "description": "Dependency injection for react applications, based on reflect-metadata",
  "keywords": [
    "react",
    "di",
    "inject",
    "container"
  ],
  "author": "Alexey <Anisimov>",
  "homepage": "https://github.com/Anissoft/js-libs#readme",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "lib/react-di.js",
  "typings": "lib/react-di",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "prepare": "npm run build",
    "prepublish": "npm run test:coverage",
    "test": "jest --env=jsdom --silent",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "jest --watch --env=jsdom"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "src",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anissoft/js-libs.git"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": ".*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "!**/*.test.(ts|tsx)",
      "src/**/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/Anissoft/js-libs/issues"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "react-dom": "^17.0.2"
  },
  "gitHead": "32dff233758cfa66cc57a55f374e16e7664c2d16"
}
