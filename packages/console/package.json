{
  "name": "@anissoft/console",
  "version": "0.0.1",
  "description": "Package to intercept all console outputs, normalize its content and bulk upload it",
  "keywords": [
    "logging",
    "log",
    "debug"
  ],
  "author": "alexey anisimov <anissoftkun@gmail.com>",
  "homepage": "https://github.com/Anissoft/js-libs#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anissoft/js-libs.git"
  },
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "prepare": "npm run build",
    "prepublish": "npm run test:coverage",
    "test": "jest --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "jest --watch --env=jsdom"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": ".*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "!**/*.test.(ts|tsx)",
      "src/**/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/Anissoft/js-libs/issues"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@types/jest": "^26.0.16",
    "jest": "26.6.3",
    "jest-config": "26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
