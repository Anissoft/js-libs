{
  "name": "@anissoft/request",
  "version": "2.0.2",
  "description": "Wrap on top of the standart fetch API with few tweaks",
  "keywords": [
    "fetch",
    "network",
    "request",
    "ajax"
  ],
  "author": "Alexey <Anisimov>",
  "homepage": "https://github.com/Anissoft/js-libs#readme",
  "license": "MIT",
  "typings": "lib/index",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anissoft/js-libs.git"
  },
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "prepare": "npm run build",
    "prepublish": "npm run test:coverage",
    "test": "jest --silent",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": ".*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "polyfill.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "!**/*.test.(ts|tsx)",
      "src/**/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/Anissoft/js-libs/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.6.3",
    "jest-config": "^25.5.3",
    "nock": "^13.0.5",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.4.1",
    "typescript": "^4.1.3"
  },
  "gitHead": "ebf723ceca1c204afb93e1bdf65d252271dd7c15"
}
